{
    "extends": "airbnb-base",
    "rules": {
      "no-multi-spaces": [0],
      "func-names": [0],
      "no-unused-vars": [0],
      "no-restricted-syntax": [0],
      "consistent-return": [0],
      "no-unused-expressions": [0],
      "comma-dangle": [0],
      "no-alert": [0],
      "no-use-before-define": [0],
      "no-useless-escape": [0]
    }
}
